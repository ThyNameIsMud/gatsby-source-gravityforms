{
  "name": "gatsby-source-gravityforms",
  "description": "Gatsby source plugin to add WordPress Gravity Forms nodes to your app.",
  "version": "0.0.1",
  "author": "Robert Marshall (https://robertmarshall.dev)",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "axios": "^0.18.0",
    "babel-preset-gatsby-package": "^0.1.4",
    "query-string": "^6.5.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-source",
    "gatsby-plugin",
    "wordpress",
    "gravityforms"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/robmarshall/gatsby-source-gravityforms.git"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "install-peers": "^1.0.3"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  }
}
