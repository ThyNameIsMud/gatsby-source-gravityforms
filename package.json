{
    "name": "gatsby-source-gravityforms",
    "description": "Gatsby source plugin to add WordPress Gravity Forms nodes to your app.",
    "version": "0.0.1",
    "author": "Robert Marshall (https://robertmarshall.dev)",
    "dependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@babel/runtime": "^7.4.4",
        "axios": "^0.18.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-gatsby-package": "^0.1.4",
        "json-stringify-safe": "^5.0.1",
        "nanoid": "^2.0.2",
        "oauth-signature": "^1.5.0"
    },
    "keywords": [
        "gatsby",
        "gatsby-source",
        "gatsby-plugin",
        "wordpress",
        "gravityforms"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/robmarshall/gatsby-source-gravityforms.git"
    },
    "devDependencies": {
        "cross-env": "^5.2.0",
        "install-peers": "^1.0.3"
    },
    "license": "MIT",
    "scripts": {
        "build": "babel src --out-dir .",
        "prepare": "cross-env NODE_ENV=production npm run build",
        "watch": "babel -w src --out-dir ."
    },
    "peerDependencies": {
        "gatsby": ">2.0.0-alpha"
    }
}
